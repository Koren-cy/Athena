  "fileRules": [
    {
      "id": "hYEov",
      "name": "电子设计",
      "match": "all",
      "conditions": [
        {
          "source": "path",
          "operator": "matches",
          "value": "[0-9]+-电子设计.*"
        }
      ],
      "enabled": true,
      "color": "orange",
      "icon": "lucide-notebook-pen"
    },
    {
      "id": "bLkGp",
      "name": "笔记规范",
